<% layout('layouts/boilerplate') %>
    <div class="d-flex flex-column align-items-center" id="download-root" data-session-id="<%=sessionId%>">
        <div class="mt-5 pt-5">
            <h4>Thank you for shopping!</h4>
            <h4 class="mt-5">Order ID: <%=order.orderNumber%>
            </h4>
        </div>
        <div>Order placed: <%=new Date(order.createdAt).toLocaleString()%>
        </div>
        <div id="status" class="mt-5 mb-3">Order processing ...</div>
        <div id="downloadList">
        </div>
    </div>

    <script src="/js/orderShow.js"></script>