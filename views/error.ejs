<% layout('layouts/boilerplate') %>
    <div class="row my-5 d-flex justify-content-center">
        <div class="col-6">
            <div class="alert alert-danger" role="alert">
                <h4 class="alert-heading text-center">
                    <%=err.message%>
                </h4>
                <% if (process.env.NODE_ENV !=='production' ) { %>
                    <p class="mb-5">
                        <%=err.stack%>
                    </p>
                    <% } %>
            </div>
        </div>
    </div>