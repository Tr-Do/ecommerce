<% layout('layouts/boilerplate') %>
    <div>
        <div><button onclick="location.href='/admin/dashboard'" class="mt-3 btn btn-success btn-lg">Dashboard</button>
        </div>
        <h1 class="text-center my-4">Users Overview</h1>
    </div>
    <table id="userList" class="text-center">
        <tr>
            <th>Username</th>
            <th>Action</th>
            <th>Confirmation</th>
        </tr>
        <tbody>
            <%users.forEach(user=> {%>
                <tr>

                    <td>
                        <%=user.username%>
                    </td>
                    <td><select name="action" id="" class="text-center">
                            <option value=delete">Delete</option>
                            <option value="reset">Change password</option>
                        </select></td>
                    <td>
                        <form action=""><button>Submit</button></form>
                    </td>

                </tr>
                <%}) %>
        </tbody>
    </table>