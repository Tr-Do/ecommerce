<% layout('layouts/boilerplate') %>

    <h1 class="text-center my-3">Users Overview</h1>
    <table id="userList" class="text-center">
        <tr>
            <th>Username</th>
            <th>Action</th>
            <th>Confirmation</th>
        </tr>
        <tbody>
            <%users.forEach(user=> {%>
                <tr>

                    <td>
                        <%=user.username%>
                    </td>
                    <td><select name="action" id="">
                            <option value=delete">Delete</option>
                            <option value="reset">Change password</option>
                        </select></td>
                    <td>
                        <form action=""><button>Submit</button></form>
                    </td>

                </tr>
                <%}) %>
        </tbody>
    </table>